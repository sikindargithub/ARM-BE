<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
      http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <configuration>
        <custom-converters>
            <converter type="io.craftsman.Jdk8CompatibilityConverter">
                <class-a>java.time.LocalDate</class-a>
                <class-b>java.time.LocalDate</class-b>
            </converter>
        </custom-converters>
    </configuration>
    <!--    ProductionCompanyName-->
    <mapping>
        <class-a>com.armapp.model.Production</class-a>
        <class-b>com.armapp.vo.ProductionVO</class-b>
        <field>
            <a>contractNumber</a>
            <b>contractNumber</b>
        </field>
        <field>
            <a>productionCompanyName</a>
            <b>productionCompanyName</b>
        </field>
    </mapping>
    <!--    ProductionCompanyName-->

    <!--    Project Name -->
    <mapping>
        <class-a>com.armapp.model.Project</class-a>
        <class-b>com.armapp.vo.ProjectVO</class-b>
        <field>
            <a>projectName</a>
            <b>projectName</b>
        </field>

    </mapping>

    <!--    Talents     -->
    <mapping>
        <class-a>com.armapp.model.Talent</class-a>
        <class-b>com.armapp.vo.TalentVO</class-b>
        <field>
            <a>talentName</a>
            <b>talentName</b>
        </field>
    </mapping>

    <!--    Request vo    -->
    <mapping>
        <class-a>com.armapp.model.Request</class-a>
        <class-b>com.armapp.vo.RequestVO</class-b>
        <field>
            <a>requestId</a>
            <b>requestId</b>
        </field>
        <field>
            <a>priority</a>
            <b>priority</b>
        </field>
        <field>
            <a>unionName</a>
            <b>union</b>
        </field>
        <field>
            <a>status</a>
            <b>status</b>
        </field>
        <field>
            <a>productionName</a>
            <b>productionCompanyName</b>
        </field>
        <field>
            <a>projectName</a>
            <b>projectName</b>
        </field>
        <field>
            <a>talentName</a>
            <b>talentName</b>
        </field>
        <field>
            <a>requestSchedule.requestCreated</a>
            <b>requestSchedule.requestCreated</b>
        </field>
        <field>
            <a>requestSchedule.expectedClosure</a>
            <b>requestSchedule.expectedClosure</b>
        </field>

    </mapping>


    <!--    CategoryVO-->
    <mapping>
        <class-a>com.armapp.model.Category</class-a>
        <class-b>com.armapp.vo.CategoryVO</class-b>
        <field>
            <a>categoryId</a>
            <b>categoryId</b>
        </field>
        <field>
            <a>reportType</a>
            <b>reportType</b>
        </field>
        <field>
            <a>owner.ownerName</a>
            <b>ownerName</b>
        </field>
    </mapping>
    <mapping>
        <class-a>com.armapp.model.ReqID</class-a>
        <class-b>com.armapp.vo.ReqIDVO</class-b>
    </mapping>

</mappings>